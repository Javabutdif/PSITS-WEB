{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" },
    {
      "source": "/api/v2/:path*",
      "destination": "https://psits-web-api.vercel.app/api/v2/:path*"
    },
    {
      "source": "/api/v2/",
      "destination": "https://psits-web-api.vercel.app/api/v2/"
    },
    {
      "source": "/auth/login/rfid",
      "destination": "https://psits-web-api.vercel.app/api/v2/auth/login/rfid"
    },
    {
      "source": "/auth/logout",
      "destination": "https://psits-web-api.vercel.app/api/v2/auth/logout"
    },
    {
      "source": "/user/current-user",
      "destination": "https://psits-web-api.vercel.app/api/v2/user/current-user"
    },
    {
      "source": "/officelog",
      "destination": "https://psits-web-api.vercel.app/api/v2/officelog"
    },
    {
      "source": "/user/:path*",
      "destination": "https://psits-web-api.vercel.app/api/v2/user/:path*"
    }
  ]
}
